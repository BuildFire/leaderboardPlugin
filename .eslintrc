{
	"extends": [
		"eslint:recommended",
		"airbnb-base"
	],
	"rules": {
		"semi": [
			2,
			"always"
		],
		"no-underscore-dangle": 0,
		"no-param-reassign": 0,
		"max-classes-per-file": 0,
		"no-unused-vars": 0,
		"no-undef": 0,
		"no-console": 0,
		"no-prototype-builtins": 0,
		"new-cap": 0,
		"block-scoped-var": 0,
		"vars-on-top": 0,
		"consistent-return": 0,
		"no-plusplus": 0,
		"max-len": 0,
		"radix": 0,
		"func-names": 0
	},
	"parserOptions": {
		"ecmaVersion": 2020,
		"sourceType": "module"
	},
	"env": {
		"browser": true,
		"node": true,
		"es6": true
	},
	"globals": {
		"buildfire": true,
		"Setting": true,
		"AuthManager": true,
		"state": true,
		"getLanguage": true,
		"Settings": true,
		"AnalyticsManager": true
	}
}
